// 30B MoE Model Deployment - Simplified
digraph {
	graph [bgcolor=white rankdir=TB]
	input [label=Input fillcolor=lightblue shape=rectangle style=filled]
	dp_split [label="Data Parallel Split" fillcolor=lightyellow shape=parallelogram style=filled]
	stage0 [label="Stage 0\n(GPUs 0-3)" fillcolor=lightblue shape=rectangle style=filled]
	stage1 [label="Stage 1\n(GPUs 4-7)" fillcolor=lightgreen shape=rectangle style=filled]
	stage2 [label="Stage 2\n(GPUs 8-11)" fillcolor=lightyellow shape=rectangle style=filled]
	stage3 [label="Stage 3\n(GPUs 12-15)" fillcolor=lightcoral shape=rectangle style=filled]
	dp_reduce [label="Data Parallel Reduce" fillcolor=lightcoral shape=ellipse style=filled]
	output [label=Output fillcolor=lightblue shape=rectangle style=filled]
	input -> dp_split
	dp_split -> stage0
	stage0 -> stage1
	stage1 -> stage2
	stage2 -> stage3
	stage3 -> dp_reduce
	dp_reduce -> output
}
