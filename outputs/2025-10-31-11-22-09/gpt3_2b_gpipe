		mb_10_comm_stage_4 [label="Micro-Batch 10\nBatch Transfer\nInput: [batch_size=1, seq_len=2048, hidden_size=2560]\nOutput: [batch_size=1, seq_len=2048, hidden_size=2560]\nFrom: R@1 To: R@2" fillcolor=lightyellow shape=parallelogram]
		mb_10_layer_21_attn_norm [label="MB10 L21\nAttention Norm\nGPU: R@2"]
		mb_10_layer_21_attn_qkv [label="MB10 L21\nAttention QKV\nGPU: R@2"]
		mb_10_layer_21_attn_out [label="MB10 L21\nAttention Out\nGPU: R@2"]
		mb_10_layer_21_attn_residual [label="MB10 L21\nAttn Residual\nGPU: R@2"]
		mb_10_layer_21_ffn_norm [label="MB10 L21\nFFN Norm\nGPU: R@2"]
		mb_10_layer_21_ffn_up [label="MB10 L21\nFFN Up\nGPU: R@2"]
		mb_10_layer_21_ffn_down [label="MB10 L21\nFFN Down\nGPU: R@2"]
		mb_10_layer_21_ffn_residual [label="MB10 L21\nFFN Residual\nGPU: R@2"]
		mb_10_comm_stage_4 -> mb_10_layer_21_attn_norm
		mb_10_layer_21_attn_norm -> mb_10_layer_21_attn_qkv
		mb_10_layer_21_attn_qkv -> mb_10_layer_21_attn_out
		mb_10_layer_21_attn_out -> mb_10_layer_21_attn_residual
		mb_10_layer_21_attn_residual -> mb_10_layer_21_ffn_norm
		mb_10_layer_21_ffn_norm -> mb_10_layer_21_ffn_up
		mb_10_layer_21_ffn_up -> mb_10_layer_21_ffn_down
		mb_10_layer_21_ffn_down -> mb_10_layer_21_ffn_residual
		mb_10_layer_22_attn_norm [label="MB10 L22\nAttention Norm\nGPU: R@2"]
		mb_10_layer_22_attn_qkv [label="MB10 L22\nAttention QKV\nGPU: R@2"]
		mb_10_layer_22_attn_out [label="MB10 L22\nAttention Out\nGPU: R@2"]
		mb_10_layer_22_attn_residual [label="MB10 L22\nAttn Residual\nGPU: R@2"]
		mb_10_layer_22_ffn_norm [label="MB10 L22\nFFN Norm\nGPU: R@2"]
		mb_10_layer_22_ffn_up [label="MB10 L22\nFFN Up\nGPU: R@2"]
		mb_10_layer_22_ffn_down [label="MB10 L22\nFFN Down\nGPU: R@2"]
		mb_10_layer_22_ffn_residual [label="MB10 L22\nFFN Residual\nGPU: R@2"]
		mb_10_layer_21_ffn_residual -> mb_10_layer_22_attn_norm
		mb_10_layer_22_attn_norm -> mb_10_layer_22_attn_qkv
		mb_10_layer_22_attn_qkv -> mb_10_layer_22_attn_out
		mb_10_layer_22_attn_out -> mb_10_layer_22_attn_residual
		mb_10_layer_22_attn_residual -> mb_10_layer_22_ffn_norm
		mb_10_layer_22_ffn_norm -> mb_10_layer_22_ffn_up
		mb_10_layer_22_ffn_up -> mb_10_layer_22_ffn_down
		mb_10_layer_22_ffn_down -> mb_10_layer_22_ffn_residual
		mb_10_layer_23_attn_norm [label="MB10 L23\nAttention Norm\nGPU: R@2"]
		mb_10_layer_23_attn_qkv [label="MB10 L23\nAttention QKV\nGPU: R@2"]
		mb_10_layer_23_attn_out [label="MB10 L23\nAttention Out\nGPU: R@2"]
		mb_10_layer_23_attn_residual [label="MB10 L23\nAttn Residual\nGPU: R@2"]
		mb_10_layer_23_ffn_norm [label="MB10 L23\nFFN Norm\nGPU: R@2"]
		mb_10_layer_23_ffn_up [label="MB10 L23\nFFN Up\nGPU: R@2"]
		mb_10_layer_23_ffn_down [label="MB10 L23\nFFN Down\nGPU: R@2"]
		mb_10_layer_23_ffn_residual [label="MB10 L23\nFFN Residual\nGPU: R@2"]
		mb_10_layer_22_ffn_residual -> mb_10_layer_23_attn_norm
		mb_10_layer_23_attn_norm -> mb_10_layer_23_attn_qkv
		mb_10_layer_23_attn_qkv -> mb_10_layer_23_attn_out
		mb_10_layer_23_attn_out -> mb_10_layer_23_attn_residual
		mb_10_layer_23_attn_residual -> mb_10_layer_23_ffn_norm
		mb_10_layer_23_ffn_norm -> mb_10_layer_23_ffn_up
		mb_10_layer_23_ffn_up -> mb_10_layer_23_ffn_down
		mb_10_layer_23_ffn_down -> mb_10_layer_23_ffn_residual
		mb_10_layer_24_attn_norm [label="MB10 L24\nAttention Norm\nGPU: R@2"]
		mb_10_layer_24_attn_qkv [label="MB10 L24\nAttention QKV\nGPU: R@2"]
		mb_10_layer_24_attn_out [label="MB10 L24\nAttention Out\nGPU: R@2"]
		mb_10_layer_24_attn_residual [label="MB10 L24\nAttn Residual\nGPU: R@2"]
		mb_10_layer_24_ffn_norm [label="MB10 L24\nFFN Norm\nGPU: R@2"]
		mb_10_layer_24_ffn_up [label="MB10 L24\nFFN Up\nGPU: R@2"]
		mb_10_layer_24_ffn_down [label="MB10 L24\nFFN Down\nGPU: R@2"]
		mb_10_layer_24_ffn_residual [label="MB10 L24\nFFN Residual\nGPU: R@2"]
		mb_10_layer_23_ffn_residual -> mb_10_layer_24_attn_norm
		mb_10_layer_24_attn_norm -> mb_10_layer_24_attn_qkv
		mb_10_layer_24_attn_qkv -> mb_10_layer_24_attn_out
		mb_10_layer_24_attn_out -> mb_10_layer_24_attn_residual
		mb_10_layer_24_attn_residual -> mb_10_layer_24_ffn_norm
		mb_10_layer_24_ffn_norm -> mb_10_layer_24_ffn_up
		mb_10_layer_24_ffn_up -> mb_10_layer_24_ffn_down
		mb_10_layer_24_ffn_down -> mb_10_layer_24_ffn_residual
		mb_10_stage_5_output [label="MB10\nStage 6\nOutput\nGPU: R@2"]
		mb_10_layer_24_ffn_residual -> mb_10_stage_5_output
		mb_11_comm_stage_4 [label="Micro-Batch 11\nBatch Transfer\nInput: [batch_size=1, seq_len=2048, hidden_size=2560]\nOutput: [batch_size=1, seq_len=2048, hidden_size=2560]\nFrom: R@1 To: R@2" fillcolor=lightyellow shape=parallelogram]
		mb_11_layer_21_attn_norm [label="MB11 L21\nAttention Norm\nGPU: R@2"]
		mb_11_layer_21_attn_qkv [label="MB11 L21\nAttention QKV\nGPU: R@2"]
		mb_11_layer_21_attn_out [label="MB11 L21\nAttention Out\nGPU: R@2"]
		mb_11_layer_21_attn_residual [label="MB11 L21\nAttn Residual\nGPU: R@2"]
		mb_11_layer_21_ffn_norm [label="MB11 L21\nFFN Norm\nGPU: R@2"]
		mb_11_layer_21_ffn_up [label="MB11 L21\nFFN Up\nGPU: R@2"]
		mb_11_layer_21_ffn_down [label="MB11 L21\nFFN Down\nGPU: R@2"]
		mb_11_layer_21_ffn_residual [label="MB11 L21\nFFN Residual\nGPU: R@2"]
		mb_11_comm_stage_4 -> mb_11_layer_21_attn_norm
		mb_11_layer_21_attn_norm -> mb_11_layer_21_attn_qkv
		mb_11_layer_21_attn_qkv -> mb_11_layer_21_attn_out
		mb_11_layer_21_attn_out -> mb_11_layer_21_attn_residual
		mb_11_layer_21_attn_residual -> mb_11_layer_21_ffn_norm
		mb_11_layer_21_ffn_norm -> mb_11_layer_21_ffn_up
		mb_11_layer_21_ffn_up -> mb_11_layer_21_ffn_down
		mb_11_layer_21_ffn_down -> mb_11_layer_21_ffn_residual
		mb_11_layer_22_attn_norm [label="MB11 L22\nAttention Norm\nGPU: R@2"]
		mb_11_layer_22_attn_qkv [label="MB11 L22\nAttention QKV\nGPU: R@2"]
		mb_11_layer_22_attn_out [label="MB11 L22\nAttention Out\nGPU: R@2"]
		mb_11_layer_22_attn_residual [label="MB11 L22\nAttn Residual\nGPU: R@2"]
		mb_11_layer_22_ffn_norm [label="MB11 L22\nFFN Norm\nGPU: R@2"]
		mb_11_layer_22_ffn_up [label="MB11 L22\nFFN Up\nGPU: R@2"]
		mb_11_layer_22_ffn_down [label="MB11 L22\nFFN Down\nGPU: R@2"]
		mb_11_layer_22_ffn_residual [label="MB11 L22\nFFN Residual\nGPU: R@2"]
		mb_11_layer_21_ffn_residual -> mb_11_layer_22_attn_norm
		mb_11_layer_22_attn_norm -> mb_11_layer_22_attn_qkv
		mb_11_layer_22_attn_qkv -> mb_11_layer_22_attn_out
		mb_11_layer_22_attn_out -> mb_11_layer_22_attn_residual
		mb_11_layer_22_attn_residual -> mb_11_layer_22_ffn_norm
		mb_11_layer_22_ffn_norm -> mb_11_layer_22_ffn_up
		mb_11_layer_22_ffn_up -> mb_11_layer_22_ffn_down
		mb_11_layer_22_ffn_down -> mb_11_layer_22_ffn_residual
		mb_11_layer_23_attn_norm [label="MB11 L23\nAttention Norm\nGPU: R@2"]
		mb_11_layer_23_attn_qkv [label="MB11 L23\nAttention QKV\nGPU: R@2"]
		mb_11_layer_23_attn_out [label="MB11 L23\nAttention Out\nGPU: R@2"]
		mb_11_layer_23_attn_residual [label="MB11 L23\nAttn Residual\nGPU: R@2"]
		mb_11_layer_23_ffn_norm [label="MB11 L23\nFFN Norm\nGPU: R@2"]
		mb_11_layer_23_ffn_up [label="MB11 L23\nFFN Up\nGPU: R@2"]
		mb_11_layer_23_ffn_down [label="MB11 L23\nFFN Down\nGPU: R@2"]
		mb_11_layer_23_ffn_residual [label="MB11 L23\nFFN Residual\nGPU: R@2"]
		mb_11_layer_22_ffn_residual -> mb_11_layer_23_attn_norm
		mb_11_layer_23_attn_norm -> mb_11_layer_23_attn_qkv
		mb_11_layer_23_attn_qkv -> mb_11_layer_23_attn_out
		mb_11_layer_23_attn_out -> mb_11_layer_23_attn_residual
		mb_11_layer_23_attn_residual -> mb_11_layer_23_ffn_norm
		mb_11_layer_23_ffn_norm -> mb_11_layer_23_ffn_up
		mb_11_layer_23_ffn_up -> mb_11_layer_23_ffn_down
		mb_11_layer_23_ffn_down -> mb_11_layer_23_ffn_residual
		mb_11_layer_24_attn_norm [label="MB11 L24\nAttention Norm\nGPU: R@2"]
		mb_11_layer_24_attn_qkv [label="MB11 L24\nAttention QKV\nGPU: R@2"]
		mb_11_layer_24_attn_out [label="MB11 L24\nAttention Out\nGPU: R@2"]
		mb_11_layer_24_attn_residual [label="MB11 L24\nAttn Residual\nGPU: R@2"]
		mb_11_layer_24_ffn_norm [label="MB11 L24\nFFN Norm\nGPU: R@2"]
		mb_11_layer_24_ffn_up [label="MB11 L24\nFFN Up\nGPU: R@2"]
		mb_11_layer_24_ffn_down [label="MB11 L24\nFFN Down\nGPU: R@2"]
		mb_11_layer_24_ffn_residual [label="MB11 L24\nFFN Residual\nGPU: R@2"]
		mb_11_layer_23_ffn_residual -> mb_11_layer_24_attn_norm
		mb_11_layer_24_attn_norm -> mb_11_layer_24_attn_qkv
		mb_11_layer_24_attn_qkv -> mb_11_layer_24_attn_out
		mb_11_layer_24_attn_out -> mb_11_layer_24_attn_residual
		mb_11_layer_24_attn_residual -> mb_11_layer_24_ffn_norm
		mb_11_layer_24_ffn_norm -> mb_11_layer_24_ffn_up
		mb_11_layer_24_ffn_up -> mb_11_layer_24_ffn_down
		mb_11_layer_24_ffn_down -> mb_11_layer_24_ffn_residual
		mb_11_stage_5_output [label="MB11\nStage 6\nOutput\nGPU: R@2"]
		mb_11_layer_24_ffn_residual -> mb_11_stage_5_output
	}
	final_comm [label="Gather Batches\nInput: [micro_batch_size=1, seq_len=2048, hidden_size=2560]\nOutput: [batch_size=12, seq_len=2048, hidden_size=2560]\nGPU: R@2" fillcolor=lightyellow shape=parallelogram]
	lm_head [label="LM Head\nInput: [batch_size=12, seq_len=2048, hidden_size=2560]\nOutput: [batch_size=12, seq_len=2048, vocab_size=50257]\nGPU: R@2" fillcolor=lightblue shape=rectangle]
	output [label="Output\nInput: [batch_size=12, seq_len=2048, vocab_size=50257]\nOutput: [batch_size=12, seq_len=2048, vocab_size=50257]\nGPU: all GPUs" fillcolor=lightgreen shape=ellipse]
	mb_0_stage_5_output -> final_comm
	mb_1_stage_5_output -> final_comm
	mb_2_stage_5_output -> final_comm
	mb_3_stage_5_output -> final_comm
	mb_4_stage_5_output -> final_comm
	mb_5_stage_5_output -> final_comm
	mb_6_stage_5_output -> final_comm
	mb_7_stage_5_output -> final_comm
	mb_8_stage_5_output -> final_comm
	mb_9_stage_5_output -> final_comm
	mb_10_stage_5_output -> final_comm
	mb_11_stage_5_output -> final_comm
	final_comm -> lm_head
	lm_head -> output
}
