// 30B MoE Model Simplified Deployment DAG
digraph {
	rankdir=LR size="12,8"
	node [shape=box style="rounded,filled"]
	input [label="Input Batch\n128 sequences" fillcolor=lightblue]
	dp [label="Data Parallel\nDP=2" fillcolor=lightblue]
	pp [label="Pipeline Parallel\nPP=2" fillcolor=lightgreen]
	ep [label="Expert Parallel\nEP=64" fillcolor=lightyellow]
	tp [label="Tensor Parallel\nTP=8" fillcolor=lightcoral]
	moe_layers [label="MoE Layers\n16 total" fillcolor=lightyellow]
	attention [label="Attention Layers\nTP optimized" fillcolor=lightcoral]
	output [label="Output\nGenerated tokens" fillcolor=lightblue]
	input -> dp
	dp -> pp
	pp -> ep
	ep -> tp
	tp -> moe_layers
	moe_layers -> attention
	attention -> output
}
