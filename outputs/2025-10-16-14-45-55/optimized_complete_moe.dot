    // ===== LAYER 2 (GPUs 8-15) =====
    subgraph cluster_layer_2 {
        label="Layer 2 (GPUs 8-15)";
        style=dashed; color=blue;

        // Attention Block
        subgraph cluster_l2_attention {
            label="Attention Block";
            style=rounded; color=lightblue;

