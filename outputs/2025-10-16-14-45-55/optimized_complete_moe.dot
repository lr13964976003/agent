    l0_attn_res -> l0_expert_route;
    l0_expert_route -> l0_exp0 -> l0_expert_agg;
    l0_expert_route -> l0_exp1 -> l0_expert_agg;
    l0_expert_route -> l0_exp2 -> l0_expert_agg;
